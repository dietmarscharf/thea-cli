{
  "name": "thea",
  "version": "1.0.0",
  "description": "PDF text extraction and analysis using Ollama vision models",
  "scripts": {
    "thea": ". venv/bin/activate && python3 thea.py --max-retries 10 -m gemma3:27b --save-image \"Belege/*.pdf\" \"Belege/*.PDF\"",
    "thea-windows": "venv\\Scripts\\activate && python thea.py --max-retries 10 -m gemma3:27b --save-image \"Belege\\*.pdf\" \"Belege\\*.PDF\"",
    "thea:kontoauszug": ". venv/bin/activate && python3 thea.py --prompt prompts/bank_konto_kontoauszuege.prompt --save-image \"Belege/*Konto*Auszug*.pdf\" \"Belege/*Konto*Auszug*.PDF\" \"Belege/*Konto*auszug*.pdf\" \"Belege/*Konto*auszug*.PDF\"",
    "thea:kontoauszug-windows": "venv\\Scripts\\activate && python thea.py --prompt prompts\\bank_konto_kontoauszuege.prompt --save-image \"Belege\\*Konto*Auszug*.pdf\" \"Belege\\*Konto*Auszug*.PDF\" \"Belege\\*Konto*auszug*.pdf\" \"Belege\\*Konto*auszug*.PDF\"",
    "setup": "python3 -m venv venv && . venv/bin/activate && pip install -r requirements.txt",
    "setup-windows": "python -m venv venv && venv\\Scripts\\activate && pip install -r requirements.txt",
    "activate": ". venv/bin/activate",
    "activate-windows": "venv\\Scripts\\activate",
    "install": ". venv/bin/activate && pip install -r requirements.txt",
    "install-windows": "venv\\Scripts\\activate && pip install -r requirements.txt",
    "build": "npm run build-$(uname -s | tr '[:upper:]' '[:lower:]')",
    "build-windows": "venv\\Scripts\\activate && pyinstaller --onefile --name thea-windows thea.py",
    "build-linux": ". venv/bin/activate && pyinstaller --onefile --name thea-linux thea.py",
    "build-macos": ". venv/bin/activate && pyinstaller --onefile --name thea-macos thea.py",
    "build-spec": ". venv/bin/activate && pyinstaller thea.spec",
    "build-spec-windows": "venv\\Scripts\\activate && pyinstaller thea.spec",
    "clean-build": "rm -rf build dist *.spec __pycache__",
    "clean-build-windows": "rmdir /s /q build dist & del *.spec & rmdir /s /q __pycache__",
    "thea:clean": ". venv/bin/activate && python3 thea.py --clean Belege/",
    "thea:clean-windows": "venv\\Scripts\\activate && python thea.py --clean Belege\\",
    "thea:clean-force": ". venv/bin/activate && python3 thea.py --clean --force Belege/",
    "thea:clean-force-windows": "venv\\Scripts\\activate && python thea.py --clean --force Belege\\",
    "thea:clean-dry": ". venv/bin/activate && python3 thea.py --clean --dry-run Belege/",
    "thea:clean-dry-windows": "venv\\Scripts\\activate && python thea.py --clean --dry-run Belege\\"
  },
  "keywords": [
    "pdf",
    "text-extraction",
    "ollama",
    "vision",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "python": ">=3.8"
  }
}